            <Label layoutX="193.0" layoutY="465.0" text="Texte de présentation en anglais:" />
            <TextArea fx:id="visitLengthFR" layoutX="75.0" layoutY="894.0" prefHeight="82.0" prefWidth="200.0" />
            <Label layoutX="86.0" layoutY="870.0" text="Durée de la visite en français:" />
            <TextArea fx:id="visitLengthEN" layoutX="354.0" layoutY="890.0" prefHeight="82.0" prefWidth="200.0" />
            <Label layoutX="362.0" layoutY="869.0" text="Durée de la visite en anglais:" />
