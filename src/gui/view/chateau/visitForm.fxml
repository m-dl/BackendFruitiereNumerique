      <Pane prefHeight="1182.0" prefWidth="600.0">
         <children>
            <Label layoutX="160.0" layoutY="97.0" text="Nom de la visite:" />
            <TextField fx:id="visitName" layoutX="292.0" layoutY="92.0" />
            <TextArea fx:id="visitPresTextFR" layoutX="32.0" layoutY="198.0" prefHeight="200.0" prefWidth="517.0" />
            <TextArea fx:id="visitPresTextEN" layoutX="35.0" layoutY="499.0" prefHeight="203.0" prefWidth="523.0" />
