            <TextArea fx:id="visitPresTextFR" layoutX="75.0" layoutY="252.0" prefHeight="200.0" prefWidth="200.0" />
            <TextArea fx:id="visitPresTextEN" layoutX="358.0" layoutY="251.0" prefHeight="203.0" prefWidth="200.0" />
